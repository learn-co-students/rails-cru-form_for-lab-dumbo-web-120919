<%= form_for(@song) do |f| %>
  <label>Name:</label><br>
  <%= f.text_field :name %><br>

  <label>Artist:</label><br>
  <%= f.collection_select :artist_id, Artist.all, :id, :name %>

  <label>Genre:</label><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  
  <%= f.submit %>
<% end %>